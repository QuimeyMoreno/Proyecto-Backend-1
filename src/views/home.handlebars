<h1>{{title}}</h1>

{{#if products.length}}
    <ul>
        {{#each products}}
            <li>
                <h2>{{this.title}}</h2>
                <p>{{this.description}}</p>
                <p>Price: ${{this.price}}</p>
                <p>Stock: {{this.stock}}</p>
                <hr>
            </li>
        {{/each}}
    </ul>
{{else}}
    <p>No se encontraron productos.</p>
{{/if}}
